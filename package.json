{
  "name": "@socialgouv/react-k8s-viewer",
  "version": "1.1.5",
  "description": "Render diagrams from your kubernetes manifests",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:ts",
    "build:ts": "npx rollup -c",
    "build:clean": "rm -rf ./dist/*",
    "test": "jest",
    "dev": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint ./src"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@juggle/resize-observer": "^3.2.0",
    "@socialgouv/eslint-config-react": "^1.46.0",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addon-storyshots": "^6.1.21",
    "@storybook/addon-storysource": "^6.1.21",
    "@storybook/react": "^6.0.28",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.14.15",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-loader": "^8.2.2",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "kubernetes-models": "^1.0.3",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "rollup": "^2.2.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.10"
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTest.ts"
    ]
  },
  "dependencies": {
    "@types/dagre": "^0.7.46",
    "dagre": "^0.8.5",
    "react-flow-renderer": "^9.6.7"
  }
}
