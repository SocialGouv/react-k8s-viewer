{
  "name": "@socialgouv/react-k8s-viewer",
  "version": "1.0.0",
  "description": "Render diagrams from your kubernetes manifests",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:ts",
    "build:ts": "npx rollup -c",
    "build:clean": "rm -rf ./dist/*",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint ."
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@socialgouv/eslint-config-react": "^1.46.0",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-essentials": "^6.0.22",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/addon-storysource": "^6.1.14",
    "@storybook/react": "^6.0.22",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.27",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.18.0",
    "jest": "^25.5.3",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "rollup": "^2.2.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@types/dagre": "^0.7.44",
    "dagre": "^0.8.5",
    "react-flow-renderer": "^8.4.1"
  }
}
